			<button type="tag" label="ATM" icon="ways_icons/atm.png" />
			<button type="tag" label="Bank" icon="ways_icons/bank.png" />
		</row>
		<row>
			<button type="tag" label="Playground" icon="ways_icons/playground.png" />
