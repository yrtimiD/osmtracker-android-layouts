			<button type="tag" label="Bench" icon="ways_icons/bench.png" />
			<button type="tag" label="Water" icon="ways_icons/drinking_water.png" />
		</row>
		<row>
			<button type="tag" label="Fuel station" icon="ways_icons/fuel_station.png" />
			<button type="tag" label="Parking" icon="ways_icons/car_parking.png" />
