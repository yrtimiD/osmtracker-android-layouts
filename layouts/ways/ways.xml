<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="htmlview.xslt"?>
<layouts>
	<layout name="root">
		<row>
			<button type="tag" label="left" icon="ways_icons/junction_left.png" />
			<button type="tag" label="cross" icon="ways_icons/junction_cross.png" />
			<button type="tag" label="right" icon="ways_icons/junction_right.png" />
		</row>
		<row>
			<button type="tag" label="cross left" icon="ways_icons/junction_cross_left.png" />
			<button type="tag" label="no cross left right" icon="ways_icons/junction_cross_left_right.png" />
			<button type="tag" label="cross right" icon="ways_icons/junction_cross_right.png" />
		</row>
		<row>
			<button type="tag" label="no left" icon="ways_icons/no_leftturn.png" />
			<button type="tag" label="no way" icon="ways_icons/noway.png" />
			<button type="tag" label="no right" icon="ways_icons/no_rightturn.png" />
			<button type="tag" label="split" icon="ways_icons/split.png" />
		</row>
		<row>
			<button type="tag" label="single way" icon="ways_icons/two_way_single.png" />
			<button type="tag" label="two way" icon="ways_icons/two_way.png" />
			<button type="tag" label="one way" icon="ways_icons/one_way.png" />
			<button type="tag" label="zebra" icon="ways_icons/zebra_crossing.png" />
		</row>
		<row>
			<button type="page" label="Ways" icon="ways_icons/way.png" targetlayout="ways" />
			<button type="page" label="Misc" icon="ways_icons/misc.png" targetlayout="misc" />
			<button type="page" label="Restrictions" icon="ways_icons/restriction.png" targetlayout="restriction" />
			<button type="page" label="Speed limits" icon="ways_icons/100.png" targetlayout="speed_limits" />
		</row>
	</layout>

	<layout name="misc">
		<row>
			<button type="voicerec" icon="logo_voice" />
			<button type="picture" icon="logo_camera" />
			<button type="textnote" icon="logo_textnote" />
			<button type="tag" label="XXXXXXXX" icon="ways_icons/delete.png" />
		</row>
		<row>
			<button type="tag" label="Bus stop LEFT" icon="ways_icons/bus_stop.png" />
			<button type="tag" label="Bus stop RIGHT" icon="ways_icons/bus_stop.png" />
		</row>
		<row>
			<button type="tag" label="Post office" icon="ways_icons/post_office.png" />
